{
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    // 建议使用单引号
    "quotes": [
      "warn",
      "single"
    ],
    // 缩进风格
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    // 多余的分号
    "no-extra-semi": [
      "warn"
    ],
    // 多余的括号
    "no-extra-parens": [
      "warn"
    ],
    // 空代码块
    "no-empty": [
      "warn"
    ],
    // 使用前未定义
    "no-use-before-define": [
      "warn",
      "nofunc"
    ],
    // 函数嵌套大于10警告
    "complexity": [
      "warn",
      10
    ],
    // 语句嵌套块太深
    "max-depth": [
      "warn",
      4
    ],
    // 回调嵌套深度
    "max-nested-callbacks": [
      "warn",
      2
    ],
    // 定义数组或对象最后多余的逗号
    "comma-dangle": [
      "error",
      "never"
    ],
    // debugger调试代码未删除
    "no-debugger": "error",
    // 允许console
    "no-console": "off",
    // 常量作为条件
    "no-constant-condition": "error",
    // 参数重复
    "no-dupe-args": "error",
    // 对象属性重复
    "no-dupe-keys": "error",
    // case重复
    "no-duplicate-case": "error",
    // 正则无法匹配任何值
    "no-empty-character-class": "error",
    // 无效正则
    "no-invalid-regexp": "error",
    // 函数被重新声明
    "no-func-assign": "error",
    // 无效的类型判断
    "valid-typeof": "error",
    // 不可能执行到的代码
    "no-unreachable": "error",
    // 行尾缺少分号
    "no-unexpected-multiline": "error",
    // 数组中多出逗号
    "no-sparse-arrays": "error",
    // 关键词与命名冲突
    "no-shadow-restricted-names": "error",
    // 变量未定义
    "no-undef": "error",
    // 变量定义后未使用
    "no-unused-vars": "error",
    // 允许条件语句中赋值操作
    "no-cond-assign": "off",
    // 禁止覆盖原生对象
    "no-native-reassign": "error",
    // 不建议使用eval
    "no-eval": "warn",
    // 禁止和NaN作比较,推荐使用isNaN方法
    "use-isnan": "warn",
    // 在else代码块中return，else是多余的
    "no-else-return": "warn",
    // '='两边不允许多个空格
    "no-multi-spaces": "warn",
    // 进制tab和space混用
    "no-mixed-spaces-and-tabs": "error",
    // object中':'前不留空格':'后留一个空格
    "key-spacing": [
      "warn",
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    // 变量定义后未使用
    "block-scoped-var": "error",
    // 函数返回值可能是不同类型
    "consistent-return": "error",
    // object getter/setter方法需要成对出现
    "accessor-pairs": "error",
    // 换行调用对象方法  点操作符应写在行首
    "dot-location": [
      "error",
      "property"
    ],
    // 多余的{}嵌套
    "no-lone-blocks": "error",
    // 无用的标记
    "no-empty-label": "off",
    // 允许扩展原生对象
    "no-extend-native": "off",
    // 浮点型需要写全 禁止.1 或 2.写法
    "no-floating-decimal": "error",
    // 声明时必须赋初值
    "init-declarations": "error",
    // 禁止在循环体中定义函数
    "no-loop-func": "error",
    // 允许new func
    "no-new-func": "off",
    // 不允与自己比较作为条件
    "no-self-compare": "error",
    // 禁止可能导致结果不明确的逗号操作符
    "no-sequences": "error",
    // 禁止抛出一个直接量 应是Error对象
    "no-throw-literal": "error",
    // 不允return时有赋值操作
    "no-return-assign": [
      "error",
      "always"
    ],
    // switch要有default
    "default-case": "error",
    // 不允许重复声明
    "no-redeclare": [
      "error",
      {
        "builtinGlobals": true
      }
    ],
    // 未使用的表达式
    "no-unused-expressions": [
      "error",
      {
        "allowShortCircuit": true,
        "allowTernary": true
      }
    ],
    // 无意义的函数call或apply
    "no-useless-call": "error",
    // 无意义的string concat
    "no-useless-concat": "error",
    // 禁用void
    "no-void": "error",
    // 禁用with
    "no-with": "error",
    // 标记未写注释
    "no-warning-comments": [
      "error",
      {
        "terms": [
          "todo"
        ],
        "location": "anywhere"
      }
    ],
    //if、else、while、for代码块用{}包围
    "curly": "error",
    // 空行最多不能超过2行
    "no-multiple-empty-lines": [
      "warn",
      {
        "max": 2
      }
    ],
    // new时必须加小括号
    "new-parens": "error",
    // 注释要加空格什么的
    "spaced-comment": "warn",
    //默认的规则要求，变量写在左边而值写在右边
    "yoda": "error",
    // 严格模式
    "strict": "warn"
  }
}
